#: ../lib/read.c:51
#, c-format
msgid "read %d byte"
msgid_plural "read %d bytes"
msgstr[0] "s'ha llegit %d byte"
msgstr[1] "s'ha llegit %d octets"

#: ../lib/read_data.c:51
msgid "read data size"
msgstr "llig la mida de les dades"

#: ../lib/read_data.c:70
#, c-format
msgid "read %lu byte of data"
