#: ../lib/read.c:51
#, c-format
msgid "read %d byte"
msgid_plural "read %d bytes"
msgstr[0] "%d byte lido"
msgstr[1] "%d bytes lidos"

#: ../lib/read_data.c:51
msgid "read data size"
msgstr "tamanho dos dados lidos"

#: ../lib/read_data.c:70
