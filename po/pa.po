#: ../src/daemon/gnuserv.c:460
msgid "Enable debugging"
msgstr "ਡੀਬੱਗਇੰਗ ਚਾਲੂ ਕਰੋ"

#: ../src/daemon/gnuserv.c:460
msgid "DEBUG"
msgstr "ਡੀਬੱਗ"

#: ../src/daemon/gnuserv.c:462
msgid "Enable verbose output"
msgstr "ਜਾਣਕਾਰੀ ਆਉਟਪੁੱਟ ਯੋਗ"

#: ../src/daemon/gnuserv.c:462
msgid "VERBOSE"
msgstr "ਜਾਣਕਾਰੀ"

