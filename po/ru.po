#: ../lib/read.c:51
#, c-format
msgid "read %d byte"
msgid_plural "read %d bytes"
msgstr[0] "прочитан %d байт"
msgstr[1] "прочитано %d байта"
msgstr[2] "прочитано %d байтов"

#: ../lib/read_data.c:51
msgid "read data size"
msgstr "размер прочитанных данных"

#: ../lib/read_data.c:70
#, c-format
msgid "read %lu byte of data"
