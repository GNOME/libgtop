#: ../lib/read.c:51
#, c-format
msgid "read %d byte"
msgid_plural "read %d bytes"
msgstr[0] "%d байт окулду"

#: ../lib/read_data.c:51
msgid "read data size"
msgstr "окулган маалымат көлөмү"

#: ../lib/read_data.c:70
#, c-format
msgid "read %lu byte of data"
msgid_plural "read %lu bytes of data"
