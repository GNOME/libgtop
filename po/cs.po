#: ../lib/read.c:51
#, c-format
msgid "read %d byte"
msgid_plural "read %d bytes"
msgstr[0] "přečten %d bajt"
msgstr[1] "přečteny %d bajty"
msgstr[2] "přečteno %d bajtů"

#: ../lib/read_data.c:51
msgid "read data size"
msgstr "velikost přečtených dat"

